{
    "name": "vaultplayer",
    "version": "1.0.0",
    "description": "Modern HLS video player with custom controls, mobile gestures, and quality switching - from AVVault",
    "main": "./dist/index.umd.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.umd.js",
            "types": "./dist/index.d.ts"
        },
        "./styles": "./dist/player.css"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "vite build",
        "dev": "vite build --watch",
        "prepublishOnly": "npm run build"
    },
    "dependencies": {
        "hls.js": "^1.5.16"
    },
    "devDependencies": {
        "terser": "^5.46.0",
        "vite": "^5.4.21"
    },
    "keywords": [
        "hls",
        "video-player",
        "html5-video",
        "streaming",
        "adaptive-streaming",
        "m3u8",
        "customizable",
        "themeable",
        "mobile-gestures"
    ],
    "author": "KhaiNguyen <KhaiNguyenIUH>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/KhaiNguyenIUH/vaultplayer.git"
    },
    "bugs": {
        "url": "https://github.com/KhaiNguyenIUH/vaultplayer/issues"
    },
    "homepage": "https://github.com/KhaiNguyenIUH/vaultplayer#readme"
}